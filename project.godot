; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Goolash"
run/main_scene="res://demo/animation/demo_animation.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Goulash="*res://addons/goulash/goulash.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/goulash/plugin.cfg", "res://addons/insta_complete/plugin.cfg")

[goulash]

animation/default_fps=12

[shader_globals]

goulash_frame={
"type": "float",
"value": 0.0
}
goulash_boil_noise={
"type": "sampler2D",
"value": "res://addons/goulash/goulash_boile_noise_texture.png"
}
